<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddCourse">

    <Spinner
        android:id="@+id/EndSemType"
        android:layout_width="214dp"
        android:layout_height="44dp"
        android:layout_marginTop="8dp"
        android:entries="@array/TypeOfSemester"
        android:spinnerMode="dropdown"
        app:layout_constraintEnd_toEndOf="@+id/StartSemType"
        app:layout_constraintStart_toStartOf="@+id/StartSemType"
        app:layout_constraintTop_toBottomOf="@+id/StartSemType" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="109dp"
        android:layout_height="34dp"
        android:text="Start Semester"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/StartSemType"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/StartSemType"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Add_Course"
        android:layout_width="282dp"
        android:layout_height="43dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="16dp"
        android:text="Add Course"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.565"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/AboutCourse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Something About Course"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.81"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.12" />

    <EditText
        android:id="@+id/CourseID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Course ID"
        app:layout_constraintEnd_toEndOf="@+id/AboutCourse"
        app:layout_constraintHorizontal_bias="0.72"
        app:layout_constraintStart_toStartOf="@+id/AboutCourse"
        app:layout_constraintTop_toBottomOf="@+id/AboutCourse" />

    <EditText
        android:id="@+id/CourseName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Name Of The Course"
        app:layout_constraintEnd_toEndOf="@+id/CourseID"
        app:layout_constraintHorizontal_bias="0.72"
        app:layout_constraintStart_toStartOf="@+id/CourseID"
        app:layout_constraintTop_toBottomOf="@+id/CourseID" />

    <EditText
        android:id="@+id/EnrollmentKey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Enrollment Key Of Course"
        app:layout_constraintEnd_toEndOf="@+id/CourseName"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/CourseName"
        app:layout_constraintTop_toBottomOf="@+id/CourseName" />

    <EditText
        android:id="@+id/Weightage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Weightage Of Course"
        app:layout_constraintEnd_toEndOf="@+id/EnrollmentKey"
        app:layout_constraintStart_toStartOf="@+id/EnrollmentKey"
        app:layout_constraintTop_toBottomOf="@+id/EnrollmentKey" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="112dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="About Course"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/AboutCourse"
        app:layout_constraintEnd_toStartOf="@+id/AboutCourse"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/AboutCourse" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="111dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:text="Course ID"
        android:textAlignment="viewStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/CourseID"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/CourseID"
        app:layout_constraintVertical_bias="0.48000002" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="113dp"
        android:layout_height="28dp"
        android:text="Course Name"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/CourseName"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/CourseName" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="113dp"
        android:layout_height="31dp"
        android:text="Enroll Key"
        android:textAlignment="viewStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/EnrollmentKey"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/EnrollmentKey"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="110dp"
        android:layout_height="29dp"
        android:layout_marginBottom="400dp"
        android:text="Weightage"
        android:textAlignment="viewStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/Weightage"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/Weightage"
        app:layout_constraintVertical_bias="0.0" />

    <Spinner
        android:id="@+id/StartSemType"
        android:layout_width="218dp"
        android:layout_height="39dp"
        android:layout_marginTop="8dp"
        android:entries="@array/TypeOfSemester"
        android:spinnerMode="dropdown"
        app:layout_constraintEnd_toEndOf="@+id/Weightage"
        app:layout_constraintHorizontal_bias="0.416"
        app:layout_constraintStart_toStartOf="@+id/Weightage"
        app:layout_constraintTop_toBottomOf="@+id/Weightage" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="116dp"
        android:layout_height="34dp"
        android:text="End Semester"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/EndSemType"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/EndSemType" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="112dp"
        android:layout_height="35dp"
        android:text="Start Session"
        android:textAlignment="viewStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/StartYear"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="@+id/StartYear" />

    <EditText
        android:id="@+id/StartYear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Name"
        app:layout_constraintEnd_toEndOf="@+id/EndSemType"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/EndSemType"
        app:layout_constraintTop_toBottomOf="@+id/EndSemType" />

    <EditText
        android:id="@+id/EndYear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Name"
        app:layout_constraintEnd_toEndOf="@+id/StartYear"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/StartYear"
        app:layout_constraintTop_toBottomOf="@+id/StartYear" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:text="End Session"
        android:textAlignment="viewStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/EndYear"
        app:layout_constraintEnd_toEndOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/EndYear" />

</android.support.constraint.ConstraintLayout>